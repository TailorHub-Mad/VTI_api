{
  "name": "base-back",
  "version": "1.0.0",
  "description": "template back",
  "main": "app.js",
  "scripts": {
    "test:coverage": "ENV_FILE=.env.test jest --config config/jest.json --collectCoverage",
    "start": "NODE_ENV=production node -r module-alias/register dist/app.js",
    "dev": "NODE_ENV=development nodemon",
    "dev:prod": "NODE_ENV=production nodemon",
    "build": "rm -rf dist && npx tsc --build tsconfig.prod.json",
    "serve": "yarn run build && NODE_ENV=production node -r module-alias/register dist/app.js",
    "seeds:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register --files ./src/seeds --env=development",
    "seeds:prod": "NODE_ENV=production ts-node -r tsconfig-paths/register --files ./src/seeds --env=production",
    "prettier": "prettier  --config .prettierrc --write src/**/*.ts",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "install:db": "bash ./scripts/dbs.sh -c -r",
    "clinic": "yarn run build && NODE_ENV=production clinic doctor --on-port 'autocannon localhost:$PORT' -- node -r module-alias/register dist/app.js",
    "heroku-postbuild": "yarn run build"
  },
  "keywords": [
    "template",
    "back"
  ],
  "_moduleAliases": {
    "@constants": "dist/constants",
    "@utils": "dist/utils",
    "@log": "dist/loaders/log.loader",
    "@errors": "dist/errors"
  },
  "author": "Tailor-Hub",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.41",
    "@types/nodemailer": "^6.4.4",
    "@types/pino": "^6.3.7",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "api-benchmark": "^1.0.1",
    "autocannon": "^7.3.0",
    "clinic": "^9.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-express": "^1.12.0",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "redis-mock": "^0.56.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/cron": "^1.7.3",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "http": "^0.0.1-security",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb-memory-server": "^6.9.6",
    "mongoose": "^5.12.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.6.5",
    "pino": "^6.11.3",
    "pino-pretty": "^4.7.1",
    "redis": "^3.1.2",
    "tslib": "^2.2.0",
    "winston": "^3.3.3"
  },
  "heroku-run-build-script": true
}
